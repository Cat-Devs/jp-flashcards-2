{
  "name": "jp-flashcards-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "sls dynamodb install",
    "dev": "pnpm codegen && next dev",
    "sls:offline": "serverless offline start",
    "sls:deploy:stage": "sls deploy --aws-profile serverless-admin --stage stage",
    "sls:remove:stage": "sls remove --aws-profile serverless-admin --stage stage",
    "sls:deploy:prod": "sls deploy --aws-profile serverless-admin --stage prod",
    "sls:remove:prod": "sls remove --aws-profile serverless-admin --stage prod",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --require dotenv/config",
    "test": "jest --watchAll --coverage",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci",
    "codegen:watch": "graphql-codegen --require dotenv/config --watch",
    "sls:localdb:migrate": "node scripts/migrate.js",
    "dynamo:upload": "node scripts/dynamo-upload.js"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@apollo/experimental-nextjs-app-support": "^0.4.3",
    "@apollo/server": "^4.9.5",
