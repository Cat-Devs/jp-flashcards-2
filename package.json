    "aws-sdk-client-mock-jest": "^2.2.0",
    "babel-jest": "^29.7.0",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "serverless": "^3.36.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^12.0.4",
    "serverless-s3-local": "^0.8.1",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "5.0.3"
